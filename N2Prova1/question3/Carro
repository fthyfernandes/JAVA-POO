package question3;

public class Carro {
	
	String marca, modelo;
	int ano;
	
	public Carro(String marca, String modelo, int ano) {
		this.marca = marca;
		this.modelo = modelo;
		this.ano = ano;
	}

	public String toString(){
		return "Marca: "+
				marca+
				". Modelo: "+
				modelo+
				". Ano: "+
				ano+
				".";
	}


	public static void main(String[] args) {
		
		Carro carro1= new Carro("Toyota","Corolla", 2018);
		
		System.out.println(carro1);
		
		
		
		

	}

}
